		"eslint": "^8.0.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"pg": "^8.7.3",
		"prettier": "^2.3.2",
		"source-map-support": "^0.5.20",
		"ts-loader": "^9.2.3",
		"ts-node": "^10.0.0",
		"tsconfig-paths": "^3.10.1",
		"typescript": "^4.3.5"
	},
	"_moduleAliases": {
		"@auth": "dist/src/auth",
		"@captcha": "dist/src/captcha",
		"@configs": "dist/src/configs",
