	"scripts": {
		"build": "nest build",
		"check": "tsc && yarn run format && yarn run lint",
		"dev": "NODE_ENV=local nest start --watch",
		"format": "prettier --write .",
		"lint": "eslint",
		"migrations:gen": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
		"migrations:gen:dev": "NODE_ENV=local ts-node ./node_modules/typeorm/cli.js migration:generate -n",
		"migrations:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
		"migrations:run:dev": "NODE_ENV=local ts-node ./node_modules/typeorm/cli.js migration:run",
		"prebuild": "rimraf dist",
		"start": "node dist/main"
	},
	"dependencies": {
		"@nestjs-modules/ioredis": "^1.0.1",
		"@nestjs-modules/mailer": "^1.8.1",
		"@nestjs/axios": "^1.0.0",
		"@nestjs/common": "^8.0.0",
		"@nestjs/config": "^1.2.0",
		"@nestjs/core": "^8.0.0",
		"@nestjs/jwt": "^9.0.0",
		"@nestjs/passport": "^9.0.0",
		"@nestjs/platform-express": "^8.0.0",
		"@nestjs/typeorm": "^8.0.3",
		"bcryptjs": "^2.4.3",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"handlebars": "^4.7.7",
