{
	"compilerOptions": {
		"allowJs": false,
		"baseUrl": "./",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"forceConsistentCasingInFileNames": false,
		"incremental": true,
		"module": "commonjs",
		"noFallthroughCasesInSwitch": false,
		"outDir": "./dist",
		"paths": {
			"@auth/*": ["src/auth/*"],
			"@captcha/*": ["src/captcha/*"],
			"@configs/*": ["src/configs/*"],
			"@decorators/*": ["src/decorators/*"],
			"@export/*": ["src/export/*"],
			"@models/*": ["src/models/*"],
			"@records/*": ["src/records/*"],
			"@template/*": ["src/template/*"],
			"@user/*": ["src/user/*"],
			"@utils/*": ["src/utils/*"]
		},
		"skipLibCheck": true,
		"strict": true,
		"strictNullChecks": false,
		"target": "es2017"
	}
}
